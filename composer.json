{
    "name": "michaldudek/flavour",
    "description": "Customizes Symfony3 for personal flavour.",
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Pałys-Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/symfony": ">=3.0,<4.0",
        "michaldudek/foundation": ">=0.9,<2.0",
        "willdurand/negotiation": "1.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd" : "~2.2"
    },
    "autoload": {
        "psr-4": {
            "MD\\Flavour\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
